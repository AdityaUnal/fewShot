    "def generate_labels(statement_file):\n",
    "    labels={}\n",
    "    df = pd.read_csv(statement_file)\n",
    "    # statements = df['statements']\n",
    "    for _,row in df.iterrows():\n",
    "        values = eval(row['statements']) \n",
    "        if len(values)!=0:\n",
    "            labels[row['ecg_id']] = values[0]\n",
    "    return labels"
   ]
